{
    "author": "Christoph Guttandin",
    "bugs": {
        "url": "https://github.com/chrisguttandin/eslint-config-holy-grail/issues"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-unicorn": "^28.0.2"
    },
    "description": "This is just my personal eslint configuration.",
    "devDependencies": {
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-angular": "^12.0.1",
        "chai": "^4.3.4",
        "commitizen": "^4.2.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.22.0",
        "grunt": "^1.3.0",
        "grunt-cli": "^1.3.2",
        "grunt-sh": "^0.2.0",
        "husky": "^4.3.8",
        "load-grunt-config": "^3.0.1",
        "mocha": "^8.3.2",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0"
    },
    "engines": {
        "node": ">=10.15.3"
    },
    "files": [
        "src/"
    ],
    "homepage": "https://github.com/chrisguttandin/eslint-config-holy-grail",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint --edit --extends @commitlint/config-angular",
            "pre-commit": "pretty-quick --config config/prettier/config.json --staged && grunt lint"
        }
    },
    "keywords": [
        "eslint",
        "eslint-config",
        "eslintconfig"
    ],
    "license": "MIT",
    "main": "src/module.js",
    "name": "eslint-config-holy-grail",
    "peerDependencies": {
        "eslint": ">=7.15.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisguttandin/eslint-config-holy-grail.git"
    },
    "scripts": {
        "test": "grunt lint && grunt test"
    },
    "version": "49.0.2"
}
